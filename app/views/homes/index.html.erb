
<!DOCTYPE html>
<html>
  <head>

    <meta charset="utf-8" />

    <!-- Set the viewport width to device width for mobile -->
    <meta name="viewport" content="width=device-width" />

    <title>Home</title>

    <%= stylesheet_link_tag "foundation.css" %>
    <%= stylesheet_link_tag "normalize.css" %>

    <script src="js/vendor/custom.modernizr.js"></script>

  </head>

  <body>

    <nav class="top-bar">
      <ul class="title-area">
        <li class="name"><h1><%= link_to "Home", root_path %></h1></li>

        <li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>
      </ul>
    </nav>
      <!-- End Nav and Banner -->

    <div class = "row">
      <div class = "large-3 column">
        <button type="button" style="height: 300px; width: 300px; border:10px;border-radius:150px; background-color:#3CF;" onClick="*">Network</button>
      </div>
      <div class = "large-5 column">
        <button type="button" style="height: 300px; width: 300px; border:10px;border-radius:150px; background-color:#F03;" onClick="*">Seminars</button>
      </div>
    </div>

      <script>
      document.write('<script src=js/vendor/' +
      ('__proto__' in {} ? 'zepto' : 'jquery') +
      '.js><\/script>')
      </script>
      <script src="js/foundation.min.js"></script>
      <script>
        $(document).foundation();
      </script>

  </body>
</html>
